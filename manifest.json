{
    "manifest_version":2,
    "name":"OpenTabs",
    "version":"1.0",
    "description": "Open URLs from textarea into separate tabs in current window",
    "permissions":["clipboardWrite","clipboardRead", "tabs"],
    "browser_action": {
        "default_icon": {                    
          "16": "images/icon16.png",          
          "32": "images/icon32.png"    ,
          "48": "images/icon48.png"        
        },
        "default_title": "Open In Tabs",
        "default_popup":"popup.html"
      },
    "icons": {
        "16": "images/icon16.png",
        "32": "images/icon32.png",
        "48": "images/icon48.png"
    }
}